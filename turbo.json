{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "prisma generate": {
      "inputs": ["prisma/schema.prisma"],
      "cache": true
    },
    "dev": {
      "dependsOn": ["^prisma generate"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build", "^prisma generate"],
      "outputs": [".next/**"],
      "cache": false
    },
    "heroku-postbuild": {
      "cache": false
    },
    "lint": {
      "outputs": [],
      "cache": false
    },
    "test:unit": {
      "outputs": [],
      "cache": false
    },
    "test:e2e:headless": {
      "outputs": [],
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "tsc": {
      "cache": false
    }
  },
  "globalEnv": [
    "ANALYZE",
    "CI",
    "CHROMATIC_APP_ID",
    "DATABASE_URL",
    "BRANCH",
    "NAMESPACE",
    "NEXTAUTH_SECRET",
    "NEXTAUTH_URL",
    "NODE_ENV",
    "PORT",
    "APP",
    "NEXT_PUBLIC_APP_NAME",
    "NEXT_PUBLIC_APP_SLUG",
    "PRODUCTION_URL",
    "PRODUCTION_APP",
    "BASE_URL",
    "NEXT_PUBLIC_SIREN_API_KEY",
    "NEXT_PUBLIC_CONTACT_EMAIL",
    "SIREN_ACCESS_TOKEN",
    "STACK_WEB_IMAGE",
    "NEXT_PUBLIC_INCLUSION_CONNECT_ISSUER",
    "NEXT_PUBLIC_INCLUSION_CONNECT_CLIENT_ID",
    "NEXT_PUBLIC_SENTRY_DSN",
    "NEXT_PUBLIC_SENTRY_ENVIRONMENT",
    "NEXT_PUBLIC_DEVELOPMENT_DISABLE_LINK_PREFETCH",
    "NEXT_PUBLIC_MATOMO_HOST",
    "NEXT_PUBLIC_MATOMO_SITE_ID",
    "EMAIL_FROM_ADDRESS",
    "EMAIL_FROM_NAME",
    "SMTP_USERNAME",
    "SMTP_PASSWORD",
    "SMTP_SERVER",
    "SMTP_PORT",
    "DATABASE_INSTANCE_NAME",
    "CONTAINER_NAMESPACE_NAME",
    "MAIN_ROOT_DOMAIN",
    "MAIN_SUBDOMAIN",
    "PREVIEW_ROOT_DOMAIN",
    "PREVIEW_SUBDOMAIN",
    "NEXT_TELEMETRY_DISABLED",
    "SENTRY_ORG",
    "SENTRY_PROJECT",
    "SENTRY_URL"
  ]
}
